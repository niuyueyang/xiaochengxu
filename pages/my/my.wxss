.my-wrapper{
  width: 100%;
  height: auto;
  overflow: hidden;
  box-sizing: border-box;
  background-color: #f0f0f0;
  min-height: 100%;
}
.my-header{
  width: 100%;
  height: 489rpx;
  overflow: hidden;
  position: relative;
  background-color: #efeff0;
}
.my-headerImg{
  display: block;
  width: 100%;
  height: 350rpx;
  overflow: hidden;
}
.my-nameWrap{
  width: 638rpx;
  height: 300rpx;
  padding-bottom: 30rpx;
  background-color: #fff;
  border-radius: 15rpx;
  position: absolute;
  left: 50%;
  margin-left: -319rpx;
  bottom:23rpx; 
   box-shadow:0 0rpx 10rpx 0rpx #07c160;/*for Android*/
  -webkit-box-shadow:0px 0px 10rpx 0rpx #07c160;/*for IOS*/
  min-height: 100rpx;
}
.my-icon{
  width: 176rpx;
  height: 176rpx;
  overflow: hidden;
  border-radius: 50%;
  position: absolute;
  left:50%;
  top: -88rpx;
  margin-left: -88rpx;
}
.my-icon image{
  width: 100%;
  height: 100%;
  display: block;
}
.my-name{
  color: #07c160;
  font-size: 20px;
  width: 100%;
  height: auto;
  overflow: hidden;
  text-align: center;
  margin-top: 118rpx;
}
.my-headerBottom{
  position: absolute;
  top: 180rpx;
  margin-top: 30rpx;
  width: 100%;
  height: auto;
  overflow: hidden;
  padding-left: 100rpx;
  padding-right: 100rpx;
  box-sizing: border-box;
  z-index: 10;

}
.my-headerItem{
  width: 50%;
  height: auto;
  overflow: hidden;
  float: left;
  text-align: center;
  color: #07c160;
}
.my-headerItem view:first-of-type{
  font-size: 38rpx;
}
.my-headerItem view:nth-of-type(2){
  font-size: 32rpx;
  color: #333333;
  margin-top: 13rpx;
}
.my-headerBottomBg{
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 132rpx;
  overflow: hidden;
  z-index: 5;
}
.my-headerBottomBg image{
  display: block;
  width: 100%;
  height: 100%;
}
.my-loginBtn{
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  background-color: #07c160;
  color: #fff;
  font-size: 32rpx;
  z-index: 999;
  position:absolute;
  top: 40rpx;
  left: 50%;
  margin-left: -100rpx;
}

/* 订单 */
.my-orderWrap{
  width: 100%;
  height: auto;
  overflow: hidden;
  background-color: #fff;
  box-sizing: border-box;
  padding-left: 31rpx;
  padding-right: 31rpx;
  padding-top: 28rpx;
}
.my-orderHeader{
  width: 100%;
  height: 51rpx;
  border-bottom:1px solid #d2d2d2;
  line-height: 51rpx;
  padding-bottom: 20rpx;
}
.my-orderTitle{
  font-size: 34rpx;
}
.my-orderAll{
  float: right;
  font-size: 28rpx;
  position: relative;
  z-index: 999;
}
.my-orderContent{
  width: 100%;
  height: 138rpx;
  box-sizing: border-box;
  padding-top: 31rpx;
  padding-bottom: 23rpx;
  overflow: hidden;
  padding-left: 42rpx;
  padding-right: 42rpx;
}
.my-orderItem{
  width: 50%;
  height: 100%;
  float: left;
  text-align: center;
}
.my-orderItem image{
  width: 44rpx;
  height: 44rpx;
  display: block;
  margin: 0 auto;
}
.my-orderItem text{
  text-align: center;
  font-size: 28rpx;
}
/* 分类 */
.my-classifyWrap{
  width: 100%;
  height: auto;
  margin-top: 20rpx;
  background-color: #fff;
  overflow: hidden;
  box-sizing: border-box;
  padding: 31rpx;
}
.my-classifyHeader{
  padding-bottom: 31rpx;
  box-sizing: border-box;
  border-bottom:1px solid #d2d2d2;
  font-size: 32rpx;
}
.my-classifyContent{
  width: 100%;
  height: auto;
  overflow: hodden;
}
.my-classifyItem{
  width: 100%;
  height: 120rpx;
  padding-top: 15rpx;
  padding-bottom: 15rpx;
  box-sizing: border-box;
}
.my-classifyItem:first-child{
  padding-top: 30rpx;
}
.my-collectImg{
  width: 44rpx;
  height: 44rpx;
  display: block;
  float: left;
  margin-top: 2rpx;
}
.my-fapiaoImg{
  width: 36rpx;
  height: 42rpx;
  display: block;
  float: left;
  margin-top: 0rpx;
}
.my-buyImg{
  width: 44rpx;
  height: 25rpx;
  display: block;
  float: left;
  margin-top: 16rpx;
}
.my-classifyItem text{
  margin-left: 14rpx;
  font-size: 30rpx;
}
.my-rightImg{
  width: 9rpx;
  height: 20rpx;
  float: right;
  display: block;
  margin-top: 10rpx;
}
.my-hover{
  background: #fff;
}